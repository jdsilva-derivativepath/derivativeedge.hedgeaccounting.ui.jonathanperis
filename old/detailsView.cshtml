<div data-ng-show="openDetailsTab">
    <div class="row search_margin">
        <div class="tradeButtonBar col-xs-12">
            <div class="col-xs-4" style="width:610px;">
                <button type="submit" class="btn btn-info" data-ng-disabled="disableSave();" data-ng-click="submit(true);">Save</button>
                <button type="button" class="btn btn-cancel" style="margin-left:10px" data-ng-disabled="InProgress" data-ng-click="cancel();">Cancel</button>
                <button type="button" class="btn btn-info" style="margin-left:10px" data-ng-disabled="disablePrevInceptionPackage();" data-ng-click="generateInceptionPackage();">Preview Inception Package</button>
                <button type="button" class="btn btn-info" style="margin-left:10px" data-ng-disabled="disableRunRegression();" data-ng-click="regress();">Run Regression</button>
                <button type="button" class="btn btn-info" style="margin-left:10px" data-ng-disabled="disableBackload();" data-ng-click="backload();">Backload</button>
            </div>
            <div class="col-xs-4 placeholder customplaceholderForHeader border1" data-ng-class="Model.ValueDate != ''? 'deactive':''" data-placeholder="Curve Date" style="width:140px;">
                <input id="curveDateInputHA" type="text" class="text-box datepickplugin" data-datetime="MM/dd/yyyy" data-ng-model="Model.ValueDate" placeholder="Curve Date" data-ng-value="Model.ValueDate" data-heightValue="33px" getIsGoodDate="true" />
            </div>
            <div class="col-xs-4" data-ng-if="Model.ShowHrSpecAndChecksButton">
                 <button type="button" class="btn btn-info" style="margin-left:10px" data-ng-click="downloadSpecAndChecks();"><i class="fa fa-download"></i></button>
            </div>
        </div>
    </div>
    @*<div class="row search_margin" data-ng-show="DesignatedIsDPIUser">
            <div class="col-xs-4 col-md-6">
                <div class="form-group">
                    <div class="col-xs-4 col-md-6 placeholder" data-placeholder="Curve Date">
                        <input type="text" class="text-box datepickplugin" data-datetime="MM/dd/yyyy" data-ng-model="Model.ValueDate" placeholder="Curve Date" data-ng-value="Model.ValueDate" />
                    </div>
                </div>
            </div>
        </div>*@

    <div class="row search_margin flex" data-ng-init="editInfoValue = false;">
        <div class="col-xs-12 col-sm-6 col-md-4 opp_revenue_bar">
            <div class="row">
                <div class="col-xs-10" style="width:calc(100% - 32px)">
                    <div class="col-xs-11" style="width:96%;">
                        <div class="row form-group" data-ng-if="IsDPIUser">
                            <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ? 'placeholder placeholderselect deactive': ''" data-placeholder="Client">
                                <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Client</div>
                                <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in AllDPIClients | filter:{'Value': Model.ClientID}:true">{{x.Text}}</div>
                                <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.ClientID" class="text-box">
                                    <option ng-repeat="x in AllDPIClients" value="{{x.Value}}">{{x.Text}}</option>
                                </select>
                            </div>
                        </div>
                        <div class="row form-group">
                            <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ? 'placeholder placeholderselect deactive': ''" data-placeholder="Entity">
                                <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Entity</div>
                                <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in BankEntitiesForDPIClient | filter:{'Value': Model.BankEntityID}:true">{{x.Text}}</div>
                                <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.BankEntityID" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                    <option ng-repeat="x in BankEntitiesForDPIClient" value="{{x.Value}}">{{x.Text}}</option>
                                </select>
                            </div>
                        </div>
                        <div class="row form-group">
                            <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ? (Model.Description != undefined && Model.Description != ''?'placeholder deactive' : 'placeholder'): ''" data-placeholder="Description">
                                <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Description</div>
                                <div class="oppInfo hedgeobjectiveDiv" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">{{Model.Description}}</div>
                                <input type="text" data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" class="text-box paddingTopInput" data-ng-model="Model.Description" placeholder="Description" data-ng-disabled="DesignatedIsDPIUser" />
                            </div>
                        </div>
                        <div class="row form-group" ng-if="!Model.IsCaarHedgeTemplate">
                            <div class="col-xs-12">
                                <div class="oppLabel">Hedging Objective</div>
                            </div>
                        </div>
                        <div class="row form-group">
                            @*inception memo template*@
                            <div class="row form-group" data-ng-show="Model.HedgeRiskType !== 'None'">
                                <div id="legacy-template" class="col-xs-12" ng-if="!Model.IsCaarHedgeTemplate" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ? 'placeholder placeholderselect deactive': ''" data-placeholder="Template">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Template</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in HedgeInceptionMemoTemplates | filter:{'Value': Model.InceptionMemoTemplateID}:true">{{x.ID}}</div>
                                    <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.InceptionMemoTemplateID" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.HedgeInceptionMemoTemplates" value="{{x.ID}}" data-ng-disabled="{{x.Disabled}}">{{x.Name}}</option>
                                    </select>
                                </div>
                                <div id="new-template" class="col-xs-12" ng-if="Model.IsCaarHedgeTemplate">
                                    <div data-ng-class="editInfoValue && DraftDesignatedIsDPIUser" class="oppLabel">{{editInfoValue && !Model.IsNewHedgeDocumentTemplate ? 'Hedging Objective' : 'Hedge Documentation'}}</div>
                                    <div ng-if="!Model.IsNewHedgeDocumentTemplate" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ? 'placeholder placeholderselect deactive': ''" data-placeholder="Template">
                                        <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in HedgeInceptionMemoTemplates | filter:{'Value': Model.InceptionMemoTemplateID}:true">{{x.ID}}</div>
                                        <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.InceptionMemoTemplateID" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                            <option ng-repeat="x in DropDownList.HedgeInceptionMemoTemplates" value="{{x.ID}}" data-ng-disabled="{{x.Disabled}}">{{x.Name}}</option>
                                        </select>
                                    </div>
                                    <div class="col-xs-12" style="display: flex; margin-top: 4px; justify-content: space-between;" ng-if="Model.IsNewHedgeDocumentTemplate">
                                        <div class="oppInfo hedgeobjectiveDiv hedge-document-tooltip" ng-if="Model.HedgeDocumentTemplateName || editInfoValue && Model.IsNewHedgeDocumentTemplate || !editInfoValue && !Model.HedgeDocumentTemplateName"
                                             style="overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical; ">
                                            {{Model.HedgeDocumentTemplateName ? Model.HedgeDocumentTemplateName : 'None'}}
                                        </div>

                                        <div class="btn-secondary" ng-if="Model.HedgeState !== 'Dedesignated' && !Model.HedgeDocumentTemplateName" style="font-size: 14px; ">
                                            <a href="#" data-ng-click="redirectToHedgeDocumentService('/hedgeaccountingapp/gallery-hrdocument?')">Select Template <i class="fa fa-external-link"></i></a>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 hedgeobjectiveDivtooltiptext hedge-document-tooltip" style="overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; "
                                         ng-if="Model.IsCaarHedgeTemplate && Model.HedgeDocumentTemplateName">
                                        <div class="oppLabel">{{Model.HedgeDocumentTemplateDescription}}</div>
                                    </div>

                                </div>
                            </div>
                            <div class="col-xs-12 hedgeobjectiveDivContainer" ng-if="!Model.IsCaarHedgeTemplate">
                                <div tooltip-enabled-onellipse
                                     class="oppInfo hedgeobjectiveDiv"
                                     data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">
                                    <span data-ng-bind-html="Model.Objective | unsafe"></span>
                                </div>
                                <span class="hedgeobjectiveDivtooltiptext"
                                      data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser"
                                      data-ng-bind-html="Model.Objective | unsafe"></span>
                                <div data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" id="detailObjectiveDiv">
                                    <textarea id="detailObjective"
                                              placeholder="Objective"
                                              data-ng-required="true"
                                              data-ng-disabled="DesignatedIsDPIUser">{{Model.Objective}}</textarea>
                                </div>
                            </div>
                            <div ng-if="Model.IsCaarHedgeTemplate">
                                <div class="col-xs-12 hedgeobjectiveDivContainer" ng-if="!Model.IsNewHedgeDocumentTemplate">
                                    <div tooltip-enabled-onellipse
                                         class="oppInfo hedgeobjectiveDiv"
                                         data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" style="max-height:60px;">
                                        <span data-ng-bind-html="Model.Objective | unsafe"></span>
                                    </div>
                                    <div data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" id="detailObjectiveDiv">
                                        <textarea id="detailObjective"
                                                  placeholder="Objective"
                                                  data-ng-required="true"
                                                  data-ng-disabled="DesignatedIsDPIUser">{{Model.Objective}}</textarea>
                                    </div>
                                </div>
                                <div class="col-xs-12 hedgeobjectiveDivContainer">
                                    <div class="btn-secondary" style="font-size: 14px; margin-top:-5px; gap: 24px; display: flex; ">
                                        <a id="preview-legacy-template" data-ng-click="previewHedgeDocumentObjective()"
                                           style="margin-top:10px;" href="#" ng-if="!Model.HedgeDocumentTemplateName && !Model.IsNewHedgeDocumentTemplate">Preview Document <i class="fa fa-external-link"></i></a>
                                        <a id="preview-new-template" href="#" data-ng-click="redirectToHedgeDocumentService('/hedgeaccountingapp/preview-hrdocument?')"
                                           ng-if="Model.HedgeDocumentTemplateName && Model.IsNewHedgeDocumentTemplate">Preview Document <i class="fa fa-external-link" href="#"></i></a>
                                        <a data-ng-class="DedesignatedHedgeDocument ? 'icon-disabled' : ''" href="#" data-ng-click="redirectToHedgeDocumentService('/hedgeaccountingapp/edit-hrdocument?')"
                                           data-ng-disabled="!editInfoValue" ng-if="Model.HedgeDocumentTemplateName">Edit Document <i class="fa fa-external-link"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group" data-ng-show="openDetailsTab">
                            <div class="col-xs-6" data-ng-show="Model.HedgeState !== 'Dedesignated'">
                                <i class="fa fa-square-o" data-type="Model.IsAnOptionHedge" data-ng-hide="Model.IsAnOptionHedge" data-ng-click="checkboxClickEvent($event)"></i>
                                <i class="fa fa-check-square" data-type="Model.IsAnOptionHedge" data-ng-show="Model.IsAnOptionHedge" data-ng-click="checkboxClickEvent($event)"></i>
                                <span class="checkboxText">Hedge is an Option</span>
                            </div>
                            <div class="col-xs-6">
                                <i class="fa fa-square-o" data-type="Model.OffMarket" data-ng-hide="Model.OffMarket &amp;&amp; !Model.IsAnOptionHedge" data-ng-click="checkboxClickEvent($event)"></i>
                                <i class="fa fa-check-square" data-type="Model.OffMarket"
                                   data-ng-show="Model.OffMarket &amp;&amp; !Model.IsAnOptionHedge" data-ng-click="checkboxClickEvent($event)"></i>
                                <span class="checkboxText">Off-Market</span>
                            </div>
                            <div class="col-xs-6" data-ng-show="Model.HedgeState !== 'Dedesignated' &amp;&amp; Model.IsHAOptIntEnabled">
                                <i class="fa fa-square-o" data-type="Model.ExcludeIntrinsicValue" data-ng-hide="Model.ExcludeIntrinsicValue" data-ng-click="checkboxClickEvent($event)"></i>
                                <i class="fa fa-check-square" data-type="Model.ExcludeIntrinsicValue" data-ng-show="Model.ExcludeIntrinsicValue" data-ng-click="checkboxClickEvent($event)"></i>
                                <span class="checkboxText">Exclude Intrinsic Value</span>
                            </div>
                            <div class="col-xs-6" data-ng-show="Model.HedgeState !== 'Dedesignated'">
                                <i class="fa fa-square-o" data-type="Model.IsDeltaMatchOption" data-ng-hide="Model.IsDeltaMatchOption" data-ng-click="checkboxClickEvent($event)"></i>
                                <i class="fa fa-check-square" data-type="Model.IsDeltaMatchOption" data-ng-show="Model.IsDeltaMatchOption" data-ng-click="checkboxClickEvent($event)"></i>
                                <span class="checkboxText">Delta Match Option</span>
                            </div>
                            <div class="col-xs-6" data-ng-show="Model.HedgeState !== 'Dedesignated' &amp;&amp; Model.ExcludeIntrinsicValue">
                                <select data-ng-model="Model.IntrinsicMethod" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                    <option ng-repeat="x in DropDownList.IntrinsicMethodList" value="{{x.Value}}">{{x.Text}}</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-2 btn-secondary" style="width:32px">
                    <i class="fa fa-pencil" data-ng-click="setFieldsFromTextBox('detail'); editInfoValue = !editInfoValue;" data-ng-show="DraftDesignatedIsDPIUser"></i>
                </div>
            </div>
        </div>
        <div class="gap col-xs-0 col-md-1"></div>
        <div class="col-xs-12 col-sm-6 col-md-4 opp_revenue_bar">
            <div class="row">
                <div class="col-xs-10" style="width:calc(100% - 32px)">
                    <div class="col-xs-6">
                        <div class="col-xs-11">
                            <div class="row form-group">
                                <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ? 'placeholder placeholderselect deactive': ''" data-placeholder="Hedged Risk">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Hedged Risk</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in DropDownList.HedgeRiskTypeList | filter:{'Value': Model.HedgeRiskType}:true">{{x.Text}}</div>
                                    <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.HedgeRiskType" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.HedgeRiskTypeList" value="{{x.Value}}">{{x.Text}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row form-group">
                                <div class="col-xs-12" data-ng-class="editInfoValue  && DraftDesignatedIsDPIUser ?'placeholder placeholderselect deactive': ''" data-placeholder="Hedged Direction">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Hedged Direction</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in DropDownList.HedgeDirectionList | filter:{'Value': Model.HedgeDirection}:true">{{x.Text}}</div>
                                    <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.HedgeDirection" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.HedgeDirectionList" value="{{x.Value}}">{{x.Text}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row form-group">
                                <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser ?'placeholder placeholderselect deactive': ''" data-placeholder="Hedge Type*">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Hedge Type</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in DropDownList.HedgeTypeList | filter:{'Value': Model.HedgeType}:true">{{x.Text}}</div>
                                    <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.HedgeType" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.HedgeTypeList" value="{{x.Value}}">{{x.Text}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'FairValue'">
                                <div class="col-xs-12" data-ng-class="editInfoValue  && DraftDesignatedIsDPIUser ?'placeholder placeholderselect deactive': ''" data-placeholder="Fair Value Method">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Fair Value Method</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in DropDownList.FairValueMethodList | filter:{'Value': Model.FairValueMethod}:true">{{x.Text}}</div>
                                    <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.FairValueMethod" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.FairValueMethodList" value="{{x.Value}}">{{x.Text}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row form-group">
                                <div class="col-xs-12" data-ng-class="editInfoValue  && DraftIsDPIUser ?'placeholder placeholderselect deactive': ''" data-placeholder="Hedging Instrument Structure">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftIsDPIUser">Hedging Instrument Structure</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftIsDPIUser" data-ng-repeat="x in DropDownList.HedgingInstrumentStructureList | filter:{'Value': Model.HedgingInstrumentStructure}:true">{{x.Text}}</div>
                                    <select data-ng-show="editInfoValue && DraftIsDPIUser" data-ng-model="Model.HedgingInstrumentStructure" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.HedgingInstrumentStructureList" value="{{x.Value}}">{{x.Text}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row form-group">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.QualitativeAssessment" data-ng-hide="Model.QualitativeAssessment" data-ng-click="checkboxClickEvent($event)"></i>
                                    <i class="fa fa-check-square" data-type="Model.QualitativeAssessment" data-ng-show="Model.QualitativeAssessment" data-ng-click="checkboxClickEvent($event)"></i>
                                    <span class="checkboxText">Qualitative Assessment</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'CashFlow' && CanEditPreIssuanceHedge">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.PreIssuanceHedge" data-ng-hide="Model.PreIssuanceHedge" data-ng-click="checkboxClickEvent($event)"></i>
                                    <i class="fa fa-check-square" data-type="Model.PreIssuanceHedge" data-ng-show="Model.PreIssuanceHedge" data-ng-click="checkboxClickEvent($event)"></i>
                                    <span class="checkboxText">Pre-Issuance Hedge</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'CashFlow' && !CanEditPreIssuanceHedge">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.PreIssuanceHedge" data-ng-hide="Model.PreIssuanceHedge" data-ng-disabled="true"></i>
                                    <i class="fa fa-check-square" data-type="Model.PreIssuanceHedge" data-ng-show="Model.PreIssuanceHedge" data-ng-disabled="true"></i>
                                    <span class="checkboxText">Pre-Issuance Hedge</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'FairValue' && CanEditPortfolioLayerMethod">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.PortfolioLayerMethod" data-ng-hide="Model.PortfolioLayerMethod" data-ng-click="checkboxClickEvent($event)"></i>
                                    <i class="fa fa-check-square" data-type="Model.PortfolioLayerMethod" data-ng-show="Model.PortfolioLayerMethod" data-ng-click="checkboxClickEvent($event)"></i>
                                    <span class="checkboxText">Portfolio Layer Method</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'FairValue' && !CanEditPortfolioLayerMethod">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.PortfolioLayerMethod" data-ng-hide="Model.PortfolioLayerMethod" data-ng-disabled="true"></i>
                                    <i class="fa fa-check-square" data-type="Model.PortfolioLayerMethod" data-ng-show="Model.PortfolioLayerMethod" data-ng-disabled="true"></i>
                                    <span class="checkboxText">Portfolio Layer Method</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'FairValue'">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.Shortcut" data-ng-hide="Model.Shortcut" data-ng-click="checkboxClickEvent($event)"></i>
                                    <i class="fa fa-check-square" data-type="Model.Shortcut" data-ng-show="Model.Shortcut" data-ng-click="checkboxClickEvent($event)"></i>
                                    <span class="checkboxText">Shortcut</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'FairValue' && Model.AssetLiability === 'Asset'">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.AvailableForSale" data-ng-hide="Model.AvailableForSale" data-ng-click="checkboxClickEvent($event)"></i>
                                    <i class="fa fa-check-square" data-type="Model.AvailableForSale" data-ng-show="Model.AvailableForSale" data-ng-click="checkboxClickEvent($event)"></i>
                                    <span class="checkboxText">Available For Sale</span>
                                </div>
                            </div>
                            <div class="row form-group" data-ng-show="Model.HedgeType === 'CashFlow'">
                                <div class="col-xs-12">
                                    <i class="fa fa-square-o" data-type="Model.Acquisition" data-ng-hide="Model.Acquisition" data-ng-click="checkboxClickEvent($event)"></i>
                                    <i class="fa fa-check-square" data-type="Model.Acquisition" data-ng-show="Model.Acquisition" data-ng-click="checkboxClickEvent($event)"></i>
                                    <span class="checkboxText">Acquisition</span>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-1"></div>
                    </div>
                    <div class="col-xs-6">
                        <div class="col-xs-1"></div>
                        <div class="col-xs-11">
                            <div class="row form-group" data-ng-show="Model.HedgeRiskType === 'InterestRate'">
                                <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser?'placeholder placeholderselect deactive': ''" data-placeholder="{{Model.BenchMarkLabel}}">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">{{Model.BenchMarkLabel}}</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in DropDownList.BenchmarkList | filter:{'Value': Model.Benchmark}:true">{{x.Text}}</div>
                                    <select data-ng-show="editInfoValue && DraftDesignatedIsDPIUser" data-ng-model="Model.Benchmark" class="text-box" data-ng-disabled="DesignatedIsDPIUser">
                                        <option ng-repeat="x in DropDownList.BenchmarkList" value="{{x.Value}}">{{x.Text}}</option>
                                    </select>
                                </div>
                            </div>


                            <div class="row form-group" data-ng-show="Model.HedgeRiskType === 'ForeignExchange' && Model.HedgeType !== 'NetInvestment'">
                                <div class="col-xs-12" data-ng-class="editInfoValue && DraftDesignatedIsDPIUser?'placeholder placeholderselect deactive': ''" data-placeholder="Exposure">
                                    <div class="oppLabel" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser">Exposure</div>
                                    <div class="oppInfo" data-ng-hide="editInfoValue && DraftDesignatedIsDPIUser" data-ng-repeat="x in DropDownList.HedgeExposureList | filter:{'Value': Model.HedgeExposure}:true">{{x.Text}}</div>
