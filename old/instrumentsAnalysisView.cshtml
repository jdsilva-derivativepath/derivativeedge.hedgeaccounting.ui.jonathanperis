<div class="row syncfusionconversion">
    <div class="col-xs-12" id="HedgedItemDiv">
    </div>
    <div class="col-xs-12" data-ng-hide="showSelectNewTrade">
        <div class="addingTradeToHedgedHedgingItemDiv" data-ng-class='Model.HedgedItems.length === 0 ? "absolute" : ""'>
            <button type="button" style="margin-right:10px;" data-ng-click="openLinkExistingTrade('HedgedItem');">Select existing trade</button>
            <div class="dropdown dropdown-reverse">
                <div class="dropdown-toggle" data-toggle="dropdown">
                    <button type="button">New trade</button>
                </div>
                <ul class="dropdown-menu">
                    <li>
                        <button type="button" data-ng-click="openNewTrade('callabledebt', 'HedgedItem')">Callable Debt</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('cancelable', 'HedgedItem')">Cancelable</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('cap', 'HedgedItem')">Cap Floor</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('collar', 'HedgedItem')">Collar</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('debt', 'HedgedItem')">Debt</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('debtoption', 'HedgedItem')">Debt Option</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('swap', 'HedgedItem')">Swap</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('swapwithoption', 'HedgedItem')">Swap With Cap/Floor</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('swaption', 'HedgedItem')">Swaption</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('corridor', 'HedgedItem')">Corridor</button>
                    </li>
                    <li>
                        <button type="button" data-ng-click="openNewTrade('fxforward', 'HedgedItem')">FX Forward</button>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

